{
	"name": "codelessshipfast",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"check:types": "turbo typecheck",
		"clean": "rimraf --glob **/node_modules **/dist **/.turbo **/.next **/.astro **/.eslintcache **/storybook-static pnpm-lock.yaml **/.tsbuildinfo",
		"dev": "next dev --turbopack",
		"format": "bunx biome format --write ./src",
		"format:check": "bunx biome check --write --unsafe ./src",
		"lint": "bunx biome lint ./src",
		"lint:fix": "bunx biome lint --fix ./src",
		"prepare": "husky",
		"start": "next start --port 9999",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"ui:add": "bunx --bun shadcn@latest add",
		"hexta:add": "bunx hexta-ui add",
		"hexta:list": "bunx hexta-ui list",
		"up:lib": "taze major -r -w -f -i"
	},
	"dependencies": {
		"@ai-sdk/deepseek": "^0.2.5",
		"@ai-sdk/groq": "^1.2.3",
		"@anthropic-ai/sdk": "^0.39.0",
		"@hookform/resolvers": "^5.0.1",
		"@radix-ui/react-avatar": "^1.1.3",
		"@radix-ui/react-checkbox": "^1.1.4",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-popover": "^1.1.6",
		"@radix-ui/react-progress": "^1.1.7",
		"@radix-ui/react-scroll-area": "^1.2.3",
		"@radix-ui/react-select": "^2.1.6",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slider": "^1.2.3",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-switch": "^1.1.3",
		"@radix-ui/react-tabs": "^1.1.3",
		"@radix-ui/react-toast": "^1.2.6",
		"@radix-ui/react-tooltip": "^1.1.8",
		"@tailwindcss/postcss": "^4.1.1",
		"@types/react-syntax-highlighter": "^15.5.13",
		"ai": "^4.2.11",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"embla-carousel-react": "^8.5.2",
		"js-yaml": "^4.1.0",
		"lucide-react": "^0.487.0",
		"next": "15.5.2",
		"next-themes": "^0.4.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-hook-form": "^7.55.0",
		"react-markdown": "^10.1.0",
		"react-resizable-panels": "^2.1.7",
		"react-syntax-highlighter": "^15.6.6",
		"sonner": "^2.0.3",
		"tailwind-merge": "^3.1.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^22.18.0",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^4.2.1",
		"husky": "^9.1.7",
		"jsdom": "^23.2.0",
		"postcss": "^8.5.3",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.1.1",
		"taze": "^19.0.4",
		"typescript": "^5.8.2",
		"vitest": "^1.3.1",
		"@vitest/coverage-v8": "^1.3.1",
		"@vitest/ui": "^1.3.1"
	}
}
