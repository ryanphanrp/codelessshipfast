{
	"name": "codelessshipfast",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"check:types": "turbo typecheck",
		"clean": "rimraf --glob **/node_modules **/dist **/.turbo **/.next **/.astro **/.eslintcache **/storybook-static pnpm-lock.yaml **/.tsbuildinfo",
		"dev": "next dev --turbopack",
		"format": "bunx biome format --write ./src",
		"format:check": "bunx biome check --write --unsafe ./src",
		"lint": "bunx biome lint ./src",
		"lint:fix": "bunx biome lint --fix ./src",
		"prepare": "husky",
		"start": "next start --port 9999",
		"ui:add": "bunx --bun shadcn@latest add",
		"up:lib": "taze major -r -w -f"
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.0",
		"@radix-ui/react-checkbox": "^1.1.4",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-popover": "^1.1.6",
		"@radix-ui/react-scroll-area": "^1.2.3",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-switch": "^1.1.3",
		"@radix-ui/react-tabs": "^1.1.3",
		"@radix-ui/react-toast": "^1.2.6",
		"@radix-ui/react-tooltip": "^1.1.8",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.0.4",
		"lucide-react": "^0.475.0",
		"next": "15.1.6",
		"next-themes": "^0.4.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.54.2",
		"react-resizable-panels": "^2.1.7",
		"sonner": "^1.7.4",
		"tailwind-merge": "^3.0.1",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/node": "^20.17.19",
		"@types/react": "^19.0.9",
		"@types/react-dom": "^19.0.3",
		"husky": "^9.1.7",
		"postcss": "^8.5.2",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.0.6",
		"taze": "^18.5.0",
		"typescript": "^5.7.3"
	}
}
